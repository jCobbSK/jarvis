{
  "name": "webpack-jarvis",
  "version": "0.3.0",
  "description": "A very intelligent webpack dashboard",
  "main": "lib/server/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "build": "webpack --env.production --config config/webpack",
    "watch": "webpack --watch --config config/webpack",
    "test": "jest",
    "integration": "concurrently --kill-others \"node cypress/mockServer/index.js\" \"PORT=7000 webpack --watch --config config/webpack\" \"cypress open\""
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zouhir/jarvis.git"
  },
  "license": "MIT",
  "author": {
    "email": "zouhir@zouhir.org",
    "name": "Zouhir",
    "url": "https://zouhir.org"
  },
  "peerDependencies": {
    "webpack": "*"
  },
  "dependencies": {
    "ansi-to-html": "^0.6.3",
    "import-from": "^2.1.0",
    "obj-str": "^1.0.0",
    "parse-authors": "^0.2.4",
    "polka": "^0.3.1",
    "react-favicon": "^0.0.11",
    "serve-static": "^1.13.1",
    "socket.io": "^2.0.3",
    "url-loader": "^0.6.2"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.10.1",
    "autoprefixer": "^7.1.2",
