    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "preact": "^8.2.1",
    "preact-compat": "^3.16.0",
    "preact-habitat": "^2.2.3",
    "preact-markup": "^1.6.0",
    "preact-render-to-json": "^3.6.6",
    "prettier": "^1.10.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.3.0",
    "style-loader": "^0.18.2",
    "svg-url-loader": "^2.1.1",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
